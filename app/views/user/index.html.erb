<h1>Пользователи и администраторы: </h1>
<table class="table table-striped">
  <thead>
  <tr>
    <td>
      Фото
    </td>
    <td>
      Имя
    </td>
    <td>
      Почта
    </td>
    <td>
      Ранг
    </td>
    <td>
      Превратить в:
    </td>
    <td></td>
    <td></td>
  </tr>
  </thead>
  <tbody id="users">
  <% @users.each do |user| %>
    <%= render partial: 'user/user', locals: {user: user} %>
  <% end %>
  </tbody>
</table>
<div class=" col-sm-offset-4 col-md-offset-3" id="pagination">
  <%= will_paginate @users, renderer: BootstrapPagination::Rails %>
</div>
